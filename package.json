{
  "name": "react-native-popup-menu",
  "version": "0.16.1",
  "description": "extensible popup/context menu for react native",
  "main": "build/rnpm.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "build": "rollup -c rollup.config.babel.js --bundleConfigAsCjs",
    "prepublish": "yarn build",
    "test": "jest",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:instea/react-native-popup-menu.git"
  },
  "author": "instea.co",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/instea/react-native-popup-menu/issues"
  },
  "homepage": "https://github.com/instea/react-native-popup-menu",
  "typings": "src/types.ts",
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "testRegex": ".*-test.js",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/examples/"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/chai",
      "<rootDir>/__tests__/helpers.js",
      "<rootDir>/node_modules/fbjs"
    ],
    "verbose": true,
    "collectCoverage": true
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/plugin-transform-class-properties": "^7.23.3",
    "@babel/preset-env": "^7.23.7",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@babel/runtime": "^7.23.7",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/react": "^18.2.46",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^29.7.0",
    "babel-polyfill": "^6.26.0",
    "chai": "^3.5.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "^3.1.1",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prop-types": "^15.5.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "^0.72.0",
    "react-test-renderer": "^18.2.0",
    "rollup": "^4.9.2",
    "sinon": "^2.2.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {}
}
